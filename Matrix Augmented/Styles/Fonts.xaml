<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Matrix_Augmented">


	<FontFamily x:Key="LatoRegular">Assets/Fonts/Lato-Regular#Lato</FontFamily>
	<FontFamily x:Key="LatoThin">Assets/Fonts/Lato-Thin.ttf#Lato</FontFamily>
	<FontFamily x:Key="LatoBold">Assets/Fonts/Lato-Bold.ttf#Lato</FontFamily>




	<x:Double x:Key="FontSizeSmallest">8</x:Double>
	<x:Double x:Key="FontSizeSmaller">10</x:Double>
	<x:Double x:Key="FontSizeSmall">12</x:Double>
	<x:Double x:Key="FontSizeRegular">14</x:Double>
	<x:Double x:Key="FontSizeLarge">20</x:Double>
	<x:Double x:Key="FontSizeXLarge">24</x:Double>
	<x:Double x:Key="FontSizeXXLarge">30</x:Double>


	


</ResourceDictionary>
